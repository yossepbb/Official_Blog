<div class="container">
	<h1 class="text-center">sign in </h1>
	  <%= form_tag admin_sessions_path do %>

		<p>
			<%= label_tag :username %> <br>
			<%= text_field_tag :username %> <br>
		</p>

		<p>
			<%= label_tag :password %> <br>
			<%= password_field_tag :password %> <br>
		</p>

		<p>
			<%= submit_tag 'log in' %>
		</p>
	<% end %>
</div>
